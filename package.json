{
  "name": "@iryu54/stack-monitor",
  "author": {
    "name": "clabroche",
    "email": "corentinlabroche@gmail.com",
    "url": "https://corentinlabroche.fr/"
  },
  "version": "1.6.18",
  "description": "Monitor processes as a stack",
  "scripts": {
    "serve": "concurrently --kill-others \"npm run serve:front\" \"npm run serve:server\"",
    "build": "vue-cli-service build --dest server/public",
    "lint": "vue-cli-service lint",
    "launchExamples": "cd example/front/ && npm ci && cd ../server && npm ci && cd .. && node ../server/bin/www ./stack.js",
    "serve:front": "vue-cli-service serve",
    "serve:server": "cd server && NODE_ENV=DEV nodemon bin/www $STACKFILE",
    "start": "server/bin/www",
    "electron": "electron ./",
    "electron:build": "electron-builder"
  },
  "build": {
    "appId": "com.electron.stackmonitor",
    "productName": "Stack Monitor",
    "mac": {
      "category": "your.app.category.type"
    },
    "linux": {
      "icon": "./electron/512x512.png",
      "executableName": "StackMonitor",
      "artifactName": "${productName}-${version}.${ext}",
      "category": "Utility"
    },
    "win": {
      "icon": "./electron/512x512.png"
    }
  },
  "main": "./electron/main.js",
  "files": [
    "server/**/*",
    ".env.production",
    "package.json"
  ],
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.3",
    "axios": "^0.21.1",
    "bluebird": "^3.7.2",
    "colors": "^1.3.3",
    "command-exists": "^1.2.9",
    "consola": "^2.15.3",
    "cookie-parser": "~1.4.4",
    "core-js": "^3.10.1",
    "cors": "^2.8.5",
    "debounce": "^1.2.1",
    "debug": "^4.3.1",
    "electron-store": "^8.0.0",
    "express": "^4.17.1",
    "fast-sort": "^2.2.0",
    "fs-extra": "^9.1.0",
    "kill-port": "^1.6.1",
    "lodash": "^4.17.21",
    "morgan": "~1.9.1",
    "new-github-issue-url": "^0.2.1",
    "open": "^6.4.0",
    "os-utils": "0.0.14",
    "pidusage-tree": "^2.0.5",
    "rxjs": "^6.6.7",
    "socket.io": "^2.4.1",
    "socket.io-client": "^2.4.0",
    "uuid": "^8.3.2",
    "vue-js-modal": "^2.0.0-rc.6",
    "vue-router": "^4.0.6",
    "vue-scroll-stop": "^0.1.5",
    "vuedraggable": "^4.0.1",
    "xterm": "^4.11.0",
    "xterm-addon-fit": "^0.5.0"
  },
  "devDependencies": {
    "@iryu54/badge-audit": "^1.0.1",
    "@types/axios": "^0.14.0",
    "@vue/cli-plugin-babel": "^4.5.4",
    "@vue/cli-plugin-eslint": "^4.5.4",
    "@vue/cli-plugin-router": "^4.5.12",
    "@vue/cli-service": "^4.5.4",
    "@vue/compiler-sfc": "^3.0.11",
    "babel-eslint": "^10.1.0",
    "concurrently": "^5.3.0",
    "electron": "^12.0.5",
    "electron-builder": "^22.10.5",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.9.0",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.7",
    "sass-loader": "^7.1.0",
    "vue": "^3.0.11",
    "vue-cli-plugin-vue-next": "~0.1.4",
    "vue-loader": "^14.2.4",
    "webpack": "^4.46.0",
    "webpack-node-externals": "^1.5.4"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "bin": {
    "stack-monitor": "server/bin/www"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/clabroche/stack-monitor.git"
  }
}
