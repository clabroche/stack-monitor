{
  "name": "@iryu54/stack-monitor",
  "version": "1.6.10",
  "scripts": {
    "serve": "concurrently --kill-others \"npm run serve:front\" \"npm run serve:server\"",
    "build": "vue-cli-service build --dest server/public",
    "lint": "vue-cli-service lint",
    "launchExamples": "cd example/front/ && npm ci && cd ../server && npm ci && cd .. && node ../server/bin/www ./stack.js",
    "serve:front": "vue-cli-service serve",
    "serve:server": "NODE_ENV=DEV nodemon server/bin/www $STACKFILE",
    "start": "server/bin/www"
  },
  "main": "server/bin/www",
  "files": [
    "server/**/*",
    ".env.production",
    "package.json"
  ],
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.13.1",
    "axios": "^0.21.1",
    "colors": "^1.3.3",
    "consola": "^2.14.0",
    "cookie-parser": "~1.4.4",
    "core-js": "^3.6.5",
    "cors": "^2.8.5",
    "debounce": "^1.2.1",
    "debug": "^4.1.1",
    "express": "^4.17.1",
    "fast-sort": "^2.2.0",
    "fs-extra": "^9.0.1",
    "kill-port": "^1.6.0",
    "lodash": "^4.17.21",
    "morgan": "~1.9.1",
    "new-github-issue-url": "^0.2.1",
    "open": "^6.4.0",
    "os-utils": "0.0.14",
    "pidusage-tree": "^2.0.5",
    "rxjs": "^6.6.0",
    "socket.io": "^2.4.1",
    "socket.io-client": "^2.3.0",
    "uuid": "^8.3.0",
    "vue-js-modal": "^2.0.0-rc.3",
    "vue-router": "^4.0.5",
    "vue-scroll-stop": "^0.1.5",
    "xterm": "^4.11.0",
    "xterm-addon-fit": "^0.5.0"
  },
  "devDependencies": {
    "@iryu54/badge-audit": "^1.0.1",
    "@types/axios": "^0.14.0",
    "@vue/cli-plugin-babel": "^4.5.4",
    "@vue/cli-plugin-eslint": "^4.5.4",
    "@vue/cli-plugin-router": "^4.5.12",
    "@vue/cli-service": "^4.5.4",
    "@vue/compiler-sfc": "^3.0.0-beta.1",
    "babel-eslint": "^10.1.0",
    "concurrently": "^5.2.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.0.0-alpha.0",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.4",
    "sass-loader": "^7.1.0",
    "vue": "^3.0.11",
    "vue-cli-plugin-vue-next": "~0.1.4",
    "vue-loader": "^14.2.4",
    "webpack": "^4.39.3",
    "webpack-node-externals": "^1.5.4"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "bin": {
    "stack-monitor": "server/bin/www"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/clabroche/stack-monitor.git"
  }
}
