{
  "name": "@iryu54/stack-monitor",
  "version": "1.8.20",
  "description": "Monitor processes as a stack",
  "author": {
    "name": "clabroche",
    "email": "corentinlabroche@gmail.com",
    "url": "https://corentinlabroche.fr/"
  },
  "scripts": {
    "serve": "concurrently --kill-others \"npm run serve:front\" \"npm run serve:server\"",
    "build": "vite build",
    "launchExamples": "cd example/front/ && npm ci && cd ../server && npm ci && cd .. && node ../server/bin/www ./stack.js",
    "serve:front": "cross-env NODE_ENV=DEV VITE_HTTP_PORT=5456  VITE_SOCKET_PORT=5457 vite",
    "serve:server": "cd server && cross-env NODE_ENV=DEV HTTP_PORT=5456  SOCKET_PORT=5457 nodemon bin/www $STACKFILE",
    "start": "server/bin/www"
  },
  "main": "./background.js",
  "files": [
    "server/**/*",
    "modules/**/*",
    ".env.production",
    "package.json"
  ],
  "dependencies": {
    "bluebird": "^3.7.2",
    "command-exists": "^1.2.9",
    "cookie-parser": "~1.4.6",
    "cors": "^2.8.5",
    "debounce": "^1.2.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "fast-sort": "^3.2.1",
    "fs-extra": "^11.1.0",
    "kill-port": "^2.0.1",
    "lodash": "^4.17.21",
    "open": "^8.4.1",
    "os-utils": "0.0.14",
    "package-json": "^8.1.0",
    "pidusage-tree": "^2.0.5",
    "ps-list": "^8.1.1",
    "semver": "^7.3.8",
    "socket.io": "^4.6.0",
    "socket.io-client": "^4.6.0",
    "typescript": "^4.9.5",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^6.3.0",
    "@types/axios": "^0.14.0",
    "@vitejs/plugin-legacy": "^4.0.1",
    "@vitejs/plugin-vue": "^4.0.0",
    "autoprefixer": "^10.4.13",
    "axios": "^1.3.3",
    "concurrently": "^7.6.0",
    "core-js": "^3.28.0",
    "cross-env": "^7.0.3",
    "jsonpath": "^1.1.1",
    "new-github-issue-url": "^1.0.0",
    "nodemon": "^2.0.20",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-visualizer": "^5.9.0",
    "sass": "^1.58.0",
    "vite": "^4.1.1",
    "vite-plugin-html": "^3.2.0",
    "vue": "^3.2.47",
    "vue-json-viewer": "^3.0.4",
    "vue-loader": "^17.0.1",
    "vue-router": "^4.1.6",
    "vuedraggable": "^4.0.1",
    "webpack": "^5.75.0",
    "xterm": "^5.1.0",
    "xterm-addon-canvas": "^0.3.0",
    "xterm-addon-fit": "^0.7.0",
    "xterm-addon-search": "^0.11.0",
    "xterm-addon-web-links": "^0.8.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "bin": {
    "stack-monitor": "server/bin/www"
  },
  "keywords": [
    "stack-monitor",
    "stack",
    "microservices",
    "git",
    "github",
    "bitbucket",
    "npm",
    "process",
    "command",
    "cli",
    "monitor",
    "monitoring"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/clabroche/stack-monitor.git"
  }
}
