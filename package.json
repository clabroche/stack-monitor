{
  "name": "@iryu54/stack-monitor",
  "version": "1.7.3",
  "description": "Monitor processes as a stack",
  "author": {
    "name": "clabroche",
    "email": "corentinlabroche@gmail.com",
    "url": "https://corentinlabroche.fr/"
  },
  "scripts": {
    "serve": "concurrently --kill-others \"npm run serve:front\" \"npm run serve:server\"",
    "build": "vue-cli-service build --dest server/public",
    "lint": "vue-cli-service lint",
    "electron": "electron ./",
    "electron:build": "vue-cli-service electron:build",
    "electron:serve": "vue-cli-service electron:serve",
    "launchExamples": "cd example/front/ && npm ci && cd ../server && npm ci && cd .. && node ../server/bin/www ./stack.js",
    "postinstall": "npx electron-builder install-app-deps || exit 0",
    "postuninstall": "npx electron-builder install-app-deps || exit 0",
    "serve:front": "NODE_ENV=DEV VUE_APP_HTTP_PORT=5456  VUE_APP_SOCKET_PORT=5457 vue-cli-service serve",
    "serve:server": "cd server && NODE_ENV=DEV HTTP_PORT=5456  SOCKET_PORT=5457 nodemon bin/www $STACKFILE",
    "start": "server/bin/www"
  },
  "main": "./background.js",
  "files": [
    "server/**/*",
    ".env.production",
    "package.json"
  ],
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.3",
    "axios": "^0.21.1",
    "bluebird": "^3.7.2",
    "colors": "^1.3.3",
    "command-exists": "^1.2.9",
    "consola": "^2.15.3",
    "cookie-parser": "~1.4.4",
    "core-js": "^3.10.1",
    "cors": "^2.8.5",
    "debounce": "^1.2.1",
    "debug": "^4.3.1",
    "electron-store": "^8.0.0",
    "express": "^4.17.1",
    "fast-sort": "^2.2.0",
    "fs-extra": "^9.1.0",
    "kill-port": "^1.6.1",
    "lodash": "^4.17.21",
    "morgan": "~1.9.1",
    "new-github-issue-url": "^0.2.1",
    "open": "^6.4.0",
    "os-utils": "0.0.14",
    "package-json": "^6.5.0",
    "pidusage-tree": "^2.0.5",
    "rxjs": "^6.6.7",
    "semver": "^7.3.5",
    "socket.io": "^2.4.1",
    "socket.io-client": "^2.4.0",
    "typescript": "^4.2.4",
    "uuid": "^8.3.2",
    "vue-js-modal": "^2.0.0-rc.6",
    "vue-router": "^4.0.6",
    "vue-scroll-stop": "^0.1.5",
    "vuedraggable": "^4.0.1",
    "xterm": "^4.11.0",
    "xterm-addon-fit": "^0.5.0"
  },
  "devDependencies": {
    "@iryu54/badge-audit": "^1.0.1",
    "@types/axios": "^0.14.0",
    "@vue/cli-plugin-babel": "^4.5.4",
    "@vue/cli-plugin-eslint": "^4.5.4",
    "@vue/cli-plugin-router": "^4.5.12",
    "@vue/cli-service": "^4.5.4",
    "@vue/compiler-sfc": "^3.0.11",
    "babel-eslint": "^10.1.0",
    "concurrently": "^5.3.0",
    "electron": "^12.0.5",
    "electron-builder": "^22.10.5",
    "electron-devtools-installer": "^3.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.9.0",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.7",
    "sass-loader": "^7.1.0",
    "shebang-loader": "0.0.1",
    "sync-rest-client": "^1.0.3",
    "vue": "^3.0.11",
    "vue-cli-plugin-electron-builder": "~2.0.0-rc.6",
    "vue-loader": "^14.2.4",
    "webpack": "^4.46.0",
    "webpack-node-externals": "^1.5.4",
    "webpack-shebang-plugin": "^1.1.4"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "bin": {
    "stack-monitor": "server/bin/www"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/clabroche/stack-monitor.git"
  },
  "keywords": [
    "stack-monitor",
    "stack",
    "microservices",
    "git",
    "github",
    "bitbucket",
    "npm",
    "process",
    "command",
    "cli",
    "monitor",
    "monitoring"
  ]
}
