{
  "name": "@iryu54/stack-monitor",
  "version": "1.4.1",
  "bin": {
    "stack-monitor": "server/bin/www"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/clabroche/stack-monitor.git"
  },
  "main": "server/bin/www",
  "files": [
    "server/**/*",
    ".env.production",
    "package.json"
  ],
  "scripts": {
    "prepublish": "npm run build",
    "postinstall": "cd server && npm i",
    "build": "vue-cli-service build --dest server/public",
    "start": "server/bin/www",
    "lint": "vue-cli-service lint",
    "serve": "NODE_ENV=DEV concurrently --kill-others \"vue-cli-service serve\" \"nodemon server/bin/www $STACKFILE\""
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.13.1",
    "axios": "^0.19.2",
    "consola": "^2.14.0",
    "socket.io-client": "^2.3.0",
    "vue-router": "^3.3.4",
    "xterm": "^4.7.0",
    "xterm-addon-fit": "^0.4.0"
  },
  "devDependencies": {
    "@iryu54/badge-audit": "^1.0.1",
    "@types/axios": "^0.14.0",
    "@vue/cli-plugin-babel": "^3.12.1",
    "@vue/cli-plugin-eslint": "^3.12.1",
    "@vue/cli-service": "^4.4.6",
    "babel-eslint": "^10.1.0",
    "concurrently": "^5.2.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "node-sass": "^4.14.1",
    "sass-loader": "^7.1.0",
    "vue": "^2.6.11",
    "vue-loader": "^14.2.4",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.39.3",
    "webpack-node-externals": "^1.5.4"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
